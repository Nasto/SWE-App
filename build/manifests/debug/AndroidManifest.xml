            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="timeSelector" android:name="yoloswagswag.swe_app.timeSelector"/>
        <activity android:label="chillActivity" android:name="yoloswagswag.swe_app.chillActivity"/>
    </application>

</manifest>
